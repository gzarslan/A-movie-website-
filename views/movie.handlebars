<section>

    <h1 id="mvHeader"> Movies</h1>
    <br>

    <div class="mv">
        {{#each MV}}
        <div class="mvProducts">
            <fieldset>
                <a id="pro" href="/products/{{this.id}}">
                    <img src="{{this.src}}" />
                    <p class="name">{{this.title}}</p>
                </a>
            </fieldset>
        </div>

        {{/each}}

    </div>


</section>